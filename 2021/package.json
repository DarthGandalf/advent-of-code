    "ts-node": "^10.4.0",
    "vue": "^3.0.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@quasar/app": "^3.2.5",
    "@quasar/quasar-app-extension-testing": "^2.0.1",
